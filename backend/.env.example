# Backend environment variables - copy to .env and fill
PORT=5000

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Clerk
# See https://clerk.com for setup. For server side, you'll typically need an API key or JWT verification
CLERK_JWT_KEY=your_clerk_jwt_key

# Frontend base URL (for CORS or notifications)
FRONTEND_BASE_URL=http://localhost:5173
